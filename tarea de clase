int levi(int M,int N){
	if(M==0){
		devoluciÃ³n N+1;
	}else if((M>0) && (N==0)){
		volver levi(M-1,1);
	}else if((M>0) && (N>0)){
	 	volver levi(M-1,levi(M,N-1));
	}
}
int main(){
	Int M,N;
	printf("Ingresa un valor para M");
	scanf("%i",&M);
	printf("Ingresa un valor para N");
	scanf("%i",&N);
	printf("El levi de %i y %i es : %i",M,N,levi(M,N));
}
